        <field name="model">wgd.vendors</field>
        <field name="priority">2</field>
        <field name="arch" type="xml">
            <tree string="Vendors">
                <field name="vendor"/>
                <field name="name"/>
                 <field name="sort_name" string="Sort Name"/>
                <field name="pay_to_vendor"/>
                <field name="phone"/>
                <field name="contact"/>
                <field name="assignee"/>
            </tree>
        </field>
    </record> 

<!-- search view of vendors  -->
    <record id="vendor_search_view" model="ir.ui.view">
        <field name="name">venodor.search.view</field>
        <field name="model">wgd.vendors</field>
        <field name="arch" type="xml">
            <search>
                <filter name="type" string="Type"/>
                <field name="vendor" string="Vendor ID" filter_domain="[('vendor','=ilike',self)]"/>
                <field name="name" string="Name" filter_domain="[('name','=ilike',self)]"/>
                <field name="sort_name" string="Sort Name" filter_domain="[('sort_name','=ilike',self)]"/>
                <field name="pay_to_vendor" string="Pay to Vendor" filter_domain="[('pay_to_vendor','=ilike',self)]"/>
                <field name="phone" string="Phone"/>
                <field name="fax" string="Fax"/>
                <field name="street" string="Address" filter_domain="[('pay_to_vendor','=ilike',self)]"/>
                <field name="city" string="City" filter_domain="[('city','=ilike',self)]"/>
                <field name="state_id" string="State" filter_domain="[('state_id','=ilike',self)]"/>
                <field name="contact" string="Contact"/>
                <field name="assignee" string="Assigned Customer" filter_domain="[('assignee','=ilike',self)]"/>
                <field name="network_id" string="Network ID"/>
                <field name="po_item_number" string="PO Item Number"/>
                <field name="edi_po" string="EDI PO"/>
                <field name="lp_month" string="Last Payment"/>
                <field name="codes" string="Codes"/>
            </search>
        </field>
    </record>

<!-- inheriting customer invoice  -->
<record id="customer_invoice_tree_inherit" model="ir.ui.view">
    <field name="name">account.out.invoice.tree.inherit</field>
    <field name="model">account.move</field>
    <field name="inherit_id" ref="account.view_out_invoice_tree"/>
    <field name="arch" type="xml">
        <field name="name" position="attributes">
            <attribute name="optional">Hide</attribute>
        </field>
        <field name="invoice_partner_display_name" position="attributes">
            <attribute name="optional">Hide</attribute>
        </field>
        <field name="activity_ids" position="attributes">
            <attribute name="optional">Hide</attribute>
        </field>
        <field name="amount_untaxed_signed" position="attributes">
            <attribute name="optional">Hide</attribute>
        </field>
        <field name="payment_state" position="attributes">
            <attribute name="optional">Hide</attribute>
        </field>
        <field name="state" position="attributes">
            <attribute name="optional">Hide</attribute>
        </field>
        <field name="ref" position="attributes">
            <attribute name="optional">Show</attribute>
        </field>

        <xpath expr="//field[@name='invoice_date']" position="before">
            <field name="company_id" string="St"/>
            <field name="name" string="Doc#"/>
            <field name="type_name" string="Type"/>
        </xpath>
        <xpath expr="//field[@name='invoice_date_due']" position="after">
            <field name="amount_total" string="Balance"/>
            <field name="discount_allowed"/>
            <field name="discount_take"/>
            <field name="allowance"/>
            <field name="amount_paid"/>
            <field name="amount_untaxed" string="Net Balance"/>
            <field name="days_past_disc"/>
            <field name="action"/>
            <field name="partner_id" string="customer id"/>
            <field name="state" string="Open Item"/>
            <field name="amount_residual_signed" string="Now Due"/>
            <field name="stmnt_disc"/>
            <field name="running_bal"/>
            <field name="stmt_disc"/>
            <field name="last_pay_date"/>
        </xpath>
        <xpath expr="//field[@name='ref']" position="after">
            <field name="ar_note"/>
        </xpath>
            
    </field>
</record>

<record id="view_company_form" model="ir.ui.view">
    <field name="name">res.company.form.inherit.vendor</field>
    <field name="model">res.company</field>
    <field name="inherit_id" ref="base.view_company_form"/>
    <field name="arch" type="xml">
        <xpath expr="/form/sheet/notebook/page[1]/group/group/field[@name='partner_id']" position="before">
            <field name="company_name"/>
        </xpath>
        <label for="name" position="replace">
            <label for="name" string="Company Code"/>
        </label>
    </field>
</record>

<record id="account.menu_action_move_out_invoice_type" model="ir.ui.menu">
    <field name="name">AR Unpaid Invoices</field>
</record>

<menuitem id="wgd_root_menu" name="Vendors" active="True" sequence="0"/>

<menuitem id="Vendors" name="Vendor Details" parent="wgd_root_menu" action="vendor_details"/>


